<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.library.controller.AdminDashboardController"
            style="-fx-background-color: #FAFAFA;">

    <!-- Top Navigation Bar -->
    <top>
        <HBox alignment="CENTER_LEFT" spacing="20"
              style="-fx-background-color: white; -fx-border-color: #E0E0E0; -fx-border-width: 0 0 1 0; -fx-padding: 15 25;">
            <Label text="ðŸ“š" style="-fx-font-size: 28px;"/>
            <Label text="Admin Dashboard" style="-fx-font-size: 20px; -fx-font-weight: bold; -fx-text-fill: #333333;"/>

            <Region HBox.hgrow="ALWAYS"/>

            <Label fx:id="welcomeLabel" text="Welcome, Admin!" style="-fx-font-size: 14px; -fx-text-fill: #666666;"/>
            <Button fx:id="logoutButton" text="Logout" onAction="#handleLogout"
                    style="-fx-background-color: transparent; -fx-text-fill: #FF8C00; -fx-font-weight: bold; -fx-cursor: hand; -fx-border-color: #FF8C00; -fx-border-radius: 5; -fx-background-radius: 5; -fx-padding: 8 20;"/>
        </HBox>
    </top>

    <!-- Left Sidebar -->
    <left>
        <VBox spacing="10" style="-fx-background-color: white; -fx-border-color: #E0E0E0; -fx-border-width: 0 1 0 0; -fx-padding: 20; -fx-min-width: 220;">
            <Label text="Navigation" style="-fx-font-size: 14px; -fx-font-weight: bold; -fx-text-fill: #999999; -fx-padding: 0 0 10 0;"/>

            <Button fx:id="stockTabButton" text="ðŸ“¦ Stock Management" onAction="#switchToStock" maxWidth="Infinity"
                    styleClass="nav-button-active"
                    style="-fx-background-color: #FFF5EE; -fx-text-fill: #FF8C00; -fx-font-size: 14px; -fx-font-weight: bold; -fx-alignment: CENTER-LEFT; -fx-padding: 12 15; -fx-background-radius: 8; -fx-cursor: hand;"/>

            <Button fx:id="librariansTabButton" text="ðŸ‘¤ Librarians" onAction="#switchToLibrarians" maxWidth="Infinity"
                    styleClass="nav-button"
                    style="-fx-background-color: transparent; -fx-text-fill: #666666; -fx-font-size: 14px; -fx-alignment: CENTER-LEFT; -fx-padding: 12 15; -fx-background-radius: 8; -fx-cursor: hand;"/>

            <Button fx:id="membersTabButton" text="ðŸ‘¥ Members" onAction="#switchToMembers" maxWidth="Infinity"
                    styleClass="nav-button"
                    style="-fx-background-color: transparent; -fx-text-fill: #666666; -fx-font-size: 14px; -fx-alignment: CENTER-LEFT; -fx-padding: 12 15; -fx-background-radius: 8; -fx-cursor: hand;"/>

            <Button fx:id="reportsTabButton" text="ðŸ“Š Reports" onAction="#switchToReports" maxWidth="Infinity"
                    styleClass="nav-button"
                    style="-fx-background-color: transparent; -fx-text-fill: #666666; -fx-font-size: 14px; -fx-alignment: CENTER-LEFT; -fx-padding: 12 15; -fx-background-radius: 8; -fx-cursor: hand;"/>

            <Separator style="-fx-padding: 10 0;"/>

            <Label text="Quick Actions" style="-fx-font-size: 14px; -fx-font-weight: bold; -fx-text-fill: #999999; -fx-padding: 10 0 10 0;"/>

            <Button text="âž• Add Book" onAction="#handleAddBook" maxWidth="Infinity"
                    style="-fx-background-color: #FF8C00; -fx-text-fill: white; -fx-font-size: 13px; -fx-font-weight: bold; -fx-padding: 10 15; -fx-background-radius: 8; -fx-cursor: hand;"/>

            <Button text="âž• Add Librarian" onAction="#handleAddLibrarian" maxWidth="Infinity"
                    style="-fx-background-color: transparent; -fx-text-fill: #FF8C00; -fx-font-size: 13px; -fx-font-weight: bold; -fx-border-color: #FF8C00; -fx-border-radius: 8; -fx-background-radius: 8; -fx-padding: 10 15; -fx-cursor: hand;"/>
        </VBox>
    </left>

    <!-- Main Content Area -->
    <center>
        <StackPane fx:id="contentPane" style="-fx-padding: 30;">

            <!-- Stock Management View -->
            <VBox fx:id="stockView" spacing="25">

                <!-- Statistics Cards -->
                <GridPane hgap="20" vgap="20">
                    <columnConstraints>
                        <ColumnConstraints percentWidth="25" hgrow="ALWAYS"/>
                        <ColumnConstraints percentWidth="25" hgrow="ALWAYS"/>
                        <ColumnConstraints percentWidth="25" hgrow="ALWAYS"/>
                        <ColumnConstraints percentWidth="25" hgrow="ALWAYS"/>
                    </columnConstraints>

                    <VBox spacing="8" GridPane.columnIndex="0"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="Total Books" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="totalBooksLabel" text="0" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #FF8C00;"/>
                    </VBox>

                    <VBox spacing="8" GridPane.columnIndex="1"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="Books Issued" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="booksIssuedLabel" text="0" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #4CAF50;"/>
                    </VBox>

                    <VBox spacing="8" GridPane.columnIndex="2"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="Available" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="booksAvailableLabel" text="0" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #2196F3;"/>
                    </VBox>

                    <VBox spacing="8" GridPane.columnIndex="3"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="Low Stock" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="lowStockLabel" text="0" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #FF4444;"/>
                    </VBox>
                </GridPane>

                <!-- Search and Actions -->
                <VBox spacing="15" style="-fx-background-color: white; -fx-background-radius: 12; -fx-padding: 25; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                    <HBox alignment="CENTER_LEFT" spacing="15">
                        <Label text="Book Inventory" style="-fx-font-size: 18px; -fx-font-weight: bold; -fx-text-fill: #333333;"/>
                        <Region HBox.hgrow="ALWAYS"/>
                        <Button text="âž• Add New Book" onAction="#handleAddBook"
                                style="-fx-background-color: #FF8C00; -fx-text-fill: white; -fx-font-weight: bold; -fx-background-radius: 8; -fx-padding: 10 20; -fx-cursor: hand; -fx-font-size: 13px;"/>
                    </HBox>

                    <HBox spacing="15" alignment="CENTER_LEFT">
                        <TextField fx:id="stockSearchField" promptText="Search by title, author, ISBN..." HBox.hgrow="ALWAYS"
                                   style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-border-color: #E0E0E0; -fx-border-width: 1.5; -fx-padding: 12; -fx-font-size: 14px;"/>

                        <Button text="Search" onAction="#handleStockSearch"
                                style="-fx-background-color: #FF8C00; -fx-text-fill: white; -fx-font-weight: bold; -fx-background-radius: 8; -fx-padding: 12 30; -fx-cursor: hand; -fx-font-size: 14px;"/>

                        <Button text="Refresh" onAction="#handleRefreshStock"
                                style="-fx-background-color: transparent; -fx-text-fill: #FF8C00; -fx-font-weight: bold; -fx-cursor: hand; -fx-border-color: #FF8C00; -fx-border-radius: 8; -fx-background-radius: 8; -fx-padding: 12 20;"/>
                    </HBox>
                </VBox>

                <!-- Stock Table -->
                <VBox spacing="15" VBox.vgrow="ALWAYS"
                      style="-fx-background-color: white; -fx-background-radius: 12; -fx-padding: 25; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                    <TableView fx:id="stockTable" VBox.vgrow="ALWAYS"
                               style="-fx-background-color: transparent; -fx-background-radius: 8;">
                        <columns>
                            <TableColumn fx:id="stockIsbnColumn" text="ISBN" prefWidth="120"/>
                            <TableColumn fx:id="stockTitleColumn" text="Title" prefWidth="250"/>
                            <TableColumn fx:id="stockAuthorColumn" text="Author" prefWidth="180"/>
                            <TableColumn fx:id="stockCategoryColumn" text="Category" prefWidth="120"/>
                            <TableColumn fx:id="stockTotalColumn" text="Total" prefWidth="80"/>
                            <TableColumn fx:id="stockAvailableColumn" text="Available" prefWidth="80"/>
                            <TableColumn fx:id="stockActionColumn" text="Actions" prefWidth="150"/>
                        </columns>
                    </TableView>
                </VBox>
            </VBox>

            <!-- Librarians View -->
            <VBox fx:id="librariansView" spacing="25" visible="false">

                <!-- Librarian Stats -->
                <HBox spacing="20">
                    <VBox spacing="8" HBox.hgrow="ALWAYS"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="Total Librarians" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="totalLibrariansLabel" text="0" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #FF8C00;"/>
                    </VBox>

                    <VBox spacing="8" HBox.hgrow="ALWAYS"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="Active Librarians" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="activeLibrariansLabel" text="0" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #4CAF50;"/>
                    </VBox>
                </HBox>

                <!-- Search Librarians -->
                <VBox spacing="15" style="-fx-background-color: white; -fx-background-radius: 12; -fx-padding: 25; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                    <HBox alignment="CENTER_LEFT" spacing="15">
                        <Label text="Librarian Management" style="-fx-font-size: 18px; -fx-font-weight: bold; -fx-text-fill: #333333;"/>
                        <Region HBox.hgrow="ALWAYS"/>
                        <Button text="âž• Add Librarian" onAction="#handleAddLibrarian"
                                style="-fx-background-color: #FF8C00; -fx-text-fill: white; -fx-font-weight: bold; -fx-background-radius: 8; -fx-padding: 10 20; -fx-cursor: hand; -fx-font-size: 13px;"/>
                    </HBox>

                    <HBox spacing="15" alignment="CENTER_LEFT">
                        <TextField fx:id="librarianSearchField" promptText="Search by name or employee ID..." HBox.hgrow="ALWAYS"
                                   style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-border-color: #E0E0E0; -fx-border-width: 1.5; -fx-padding: 12; -fx-font-size: 14px;"/>

                        <Button text="Search" onAction="#handleLibrarianSearch"
                                style="-fx-background-color: #FF8C00; -fx-text-fill: white; -fx-font-weight: bold; -fx-background-radius: 8; -fx-padding: 12 30; -fx-cursor: hand; -fx-font-size: 14px;"/>
                    </HBox>
                </VBox>

                <!-- Librarians Table -->
                <VBox spacing="15" VBox.vgrow="ALWAYS"
                      style="-fx-background-color: white; -fx-background-radius: 12; -fx-padding: 25; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                    <TableView fx:id="librariansTable" VBox.vgrow="ALWAYS"
                               style="-fx-background-color: transparent; -fx-background-radius: 8;">
                        <columns>
                            <TableColumn fx:id="libIdColumn" text="Employee ID" prefWidth="120"/>
                            <TableColumn fx:id="libNameColumn" text="Name" prefWidth="200"/>
                            <TableColumn fx:id="libEmailColumn" text="Email" prefWidth="220"/>
                            <TableColumn fx:id="libPhoneColumn" text="Phone" prefWidth="140"/>
                            <TableColumn fx:id="libJoinDateColumn" text="Join Date" prefWidth="120"/>
                            <TableColumn fx:id="libStatusColumn" text="Status" prefWidth="100"/>
                            <TableColumn fx:id="libActionColumn" text="Actions" prefWidth="150"/>
                        </columns>
                    </TableView>
                </VBox>
            </VBox>

            <!-- Members View -->
            <VBox fx:id="membersView" spacing="25" visible="false">

                <!-- Member Stats -->
                <HBox spacing="20">
                    <VBox spacing="8" HBox.hgrow="ALWAYS"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="Total Members" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="totalMembersLabel" text="0" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #FF8C00;"/>
                    </VBox>

                    <VBox spacing="8" HBox.hgrow="ALWAYS"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="Active Members" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="activeMembersLabel" text="0" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #4CAF50;"/>
                    </VBox>

                    <VBox spacing="8" HBox.hgrow="ALWAYS"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="Pending Applications" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="pendingApplicationsLabel" text="0" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #2196F3;"/>
                    </VBox>
                </HBox>

                <!-- Search Members -->
                <VBox spacing="15" style="-fx-background-color: white; -fx-background-radius: 12; -fx-padding: 25; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                    <Label text="Member Management" style="-fx-font-size: 18px; -fx-font-weight: bold; -fx-text-fill: #333333;"/>

                    <HBox spacing="15" alignment="CENTER_LEFT">
                        <TextField fx:id="memberSearchField" promptText="Search by name, email, or member ID..." HBox.hgrow="ALWAYS"
                                   style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-border-color: #E0E0E0; -fx-border-width: 1.5; -fx-padding: 12; -fx-font-size: 14px;"/>

                        <Button text="Search" onAction="#handleMemberSearch"
                                style="-fx-background-color: #FF8C00; -fx-text-fill: white; -fx-font-weight: bold; -fx-background-radius: 8; -fx-padding: 12 30; -fx-cursor: hand; -fx-font-size: 14px;"/>
                    </HBox>
                </VBox>

                <!-- Members Table -->
                <VBox spacing="15" VBox.vgrow="ALWAYS"
                      style="-fx-background-color: white; -fx-background-radius: 12; -fx-padding: 25; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                    <TableView fx:id="adminMembersTable" VBox.vgrow="ALWAYS"
                               style="-fx-background-color: transparent; -fx-background-radius: 8;">
                        <columns>
                            <TableColumn fx:id="adminMemberIdColumn" text="Member ID" prefWidth="100"/>
                            <TableColumn fx:id="adminMemberNameColumn" text="Name" prefWidth="180"/>
                            <TableColumn fx:id="adminMemberEmailColumn" text="Email" prefWidth="200"/>
                            <TableColumn fx:id="adminMemberPhoneColumn" text="Phone" prefWidth="140"/>
                            <TableColumn fx:id="adminMemberJoinDateColumn" text="Join Date" prefWidth="120"/>
                            <TableColumn fx:id="adminMemberBooksColumn" text="Books Out" prefWidth="100"/>
                            <TableColumn fx:id="adminMemberStatusColumn" text="Status" prefWidth="100"/>
                            <TableColumn fx:id="adminMemberActionColumn" text="Actions" prefWidth="150"/>
                        </columns>
                    </TableView>
                </VBox>
            </VBox>

            <!-- Reports View -->
            <VBox fx:id="reportsView" spacing="25" visible="false">

                <!-- Report Type Selection -->
                <VBox spacing="15" style="-fx-background-color: white; -fx-background-radius: 12; -fx-padding: 25; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                    <Label text="Generate Reports" style="-fx-font-size: 18px; -fx-font-weight: bold; -fx-text-fill: #333333;"/>

                    <HBox spacing="15" alignment="CENTER_LEFT">
                        <ComboBox fx:id="reportTypeCombo" promptText="Select report type" prefWidth="250"
                                  style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-border-color: #E0E0E0; -fx-border-width: 1.5; -fx-padding: 8;">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="Books Issued Report"/>
                                    <String fx:value="Overdue Books Report"/>
                                    <String fx:value="Member Activity Report"/>
                                    <String fx:value="Popular Books Report"/>
                                    <String fx:value="Inventory Status Report"/>
                                    <String fx:value="Revenue Report"/>
                                </FXCollections>
                            </items>
                        </ComboBox>

                        <Label text="From:" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <DatePicker fx:id="reportStartDate" promptText="Start Date" prefWidth="140"
                                    style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-border-color: #E0E0E0; -fx-border-width: 1.5;"/>

                        <Label text="To:" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <DatePicker fx:id="reportEndDate" promptText="End Date" prefWidth="140"
                                    style="-fx-background-radius: 8; -fx-border-radius: 8; -fx-border-color: #E0E0E0; -fx-border-width: 1.5;"/>

                        <Region HBox.hgrow="ALWAYS"/>

                        <Button text="Generate" onAction="#handleGenerateReport"
                                style="-fx-background-color: #FF8C00; -fx-text-fill: white; -fx-font-weight: bold; -fx-background-radius: 8; -fx-padding: 12 30; -fx-cursor: hand; -fx-font-size: 14px;"/>

                        <Button text="Export PDF" onAction="#handleExportPDF"
                                style="-fx-background-color: transparent; -fx-text-fill: #FF8C00; -fx-font-weight: bold; -fx-cursor: hand; -fx-border-color: #FF8C00; -fx-border-radius: 8; -fx-background-radius: 8; -fx-padding: 12 20;"/>
                    </HBox>
                </VBox>

                <!-- Report Summary Cards -->
                <GridPane hgap="20" vgap="20">
                    <columnConstraints>
                        <ColumnConstraints percentWidth="33.33" hgrow="ALWAYS"/>
                        <ColumnConstraints percentWidth="33.33" hgrow="ALWAYS"/>
                        <ColumnConstraints percentWidth="33.33" hgrow="ALWAYS"/>
                    </columnConstraints>

                    <VBox spacing="8" GridPane.columnIndex="0"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="Total Transactions" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="totalTransactionsLabel" text="0" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #FF8C00;"/>
                        <Label text="This Month" style="-fx-text-fill: #999999; -fx-font-size: 11px;"/>
                    </VBox>

                    <VBox spacing="8" GridPane.columnIndex="1"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="Active Users" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="activeUsersLabel" text="0" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #4CAF50;"/>
                        <Label text="This Week" style="-fx-text-fill: #999999; -fx-font-size: 11px;"/>
                    </VBox>

                    <VBox spacing="8" GridPane.columnIndex="2"
                          style="-fx-background-color: white; -fx-background-radius: 10; -fx-padding: 20; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                        <Label text="System Uptime" style="-fx-text-fill: #666666; -fx-font-size: 13px;"/>
                        <Label fx:id="systemUptimeLabel" text="99.9%" style="-fx-font-size: 32px; -fx-font-weight: bold; -fx-text-fill: #2196F3;"/>
                        <Label text="Last 30 Days" style="-fx-text-fill: #999999; -fx-font-size: 11px;"/>
                    </VBox>
                </GridPane>

                <!-- Report Data Display -->
                <VBox spacing="15" VBox.vgrow="ALWAYS"
                      style="-fx-background-color: white; -fx-background-radius: 12; -fx-padding: 25; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.08), 8, 0, 0, 2);">
                    <HBox alignment="CENTER_LEFT" spacing="15">
                        <Label text="Report Data" style="-fx-font-size: 18px; -fx-font-weight: bold; -fx-text-fill: #333333;"/>
                        <Region HBox.hgrow="ALWAYS"/>
                        <Label fx:id="reportStatusLabel" text="No report generated" style="-fx-text-fill: #999999; -fx-font-size: 13px; -fx-font-style: italic;"/>
                    </HBox>

                    <TableView fx:id="reportDataTable" VBox.vgrow="ALWAYS"
                               style="-fx-background-color: transparent; -fx-background-radius: 8;">
                        <columns>
                            <TableColumn fx:id="reportCol1" text="Column 1" prefWidth="150"/>
                            <TableColumn fx:id="reportCol2" text="Column 2" prefWidth="150"/>
                            <TableColumn fx:id="reportCol3" text="Column 3" prefWidth="150"/>
                            <TableColumn fx:id="reportCol4" text="Column 4" prefWidth="150"/>
                            <TableColumn fx:id="reportCol5" text="Column 5" prefWidth="150"/>
                        </columns>
                    </TableView>
                </VBox>
            </VBox>
        </StackPane>
    </center>

    <stylesheets>
        <String fx:value="/styles.css"/>
    </stylesheets>
</BorderPane>